{% extends 'bootstrap.html.twig' %}

{% block body %}
    <header class='header'>
        <div class="container text-center" >
            <h1 class="text-center">{{'home.title'|trans}}</h1><hr class="text-center"/>
            <div class='row'> 

                <p class="text-center">{{'home.description'|trans}}</p>
            </div>
            <div class='row'><script async 
                src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
                <!-- frAnnuaire -->
                <ins class="adsbygoogle"
                     style="display:block"
                     data-ad-client="ca-pub-9603593050456448"
                     data-ad-slot="5109858557"
                     data-ad-format="auto"></ins>
                <script>
                    (adsbygoogle = window.adsbygoogle || []).push({});
                </script></div> 
        </div>
    </header>

    <div class="container">
        <h2>{{'search.h2'|trans({'%keys%': keys, '%nbRes%':nbRes})|raw }}</h2>
        {% set vars = {'webSites': webSites} %}
        {% include 'annuaire/categorySite.html.twig' with vars %}

    </div>  




{% endblock %}


